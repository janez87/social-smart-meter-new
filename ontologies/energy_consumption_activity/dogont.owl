        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2008-01-01</terms:issued>
        <vann:preferredNamespaceUri rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://elite.polito.it/ontologies/dogont</vann:preferredNamespaceUri>
        <terms:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dario Bonino (dario.bonino@gmail.com)</terms:creator>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2017-11-03</terms:modified>
